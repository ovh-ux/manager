{
  "name": "@ovh-ux/manager-kubernetes-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "webpack",
    "cypress:open": "cypress open",
    "dev": "webpack-dev-server",
    "dev:watch": "yarn run dev",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-kubernetes-app' --include-filtered-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-kubernetes-app' --include-filtered-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-kubernetes-app' --include-filtered-dependencies -- yarn run dev:watch",
    "test:e2e": "lerna exec --scope @ovh-ux/manager-kubernetes-app -- node \\$LERNA_ROOT_PATH/scripts/webpack-cypress"
  },
  "dependencies": {
    "@ovh-ux/manager-cloud-styles": "^0.3.0-alpha.0",
    "@ovh-ux/manager-config": "^0.2.0",
    "@ovh-ux/manager-core": "^5.0.0",
    "@ovh-ux/manager-kubernetes": "^0.1.0-alpha.0",
    "@ovh-ux/ng-ovh-cloud-universe-components": "^1.1.0-alpha.0",
    "@ovh-ux/ng-ovh-proxy-request": "^1.0.0-beta.0",
    "@ovh-ux/ng-ovh-user-pref": "^1.0.0",
    "@ovh-ux/ng-translate-async-loader": "^2.0.0",
    "angular": "^1.7.5",
    "angular-ui-bootstrap": "^1.3.3",
    "font-awesome": "^4.7.0",
    "ovh-api-services": "^6.8.0",
    "ovh-ui-angular": "^2.28.0",
    "ovh-ui-kit": "^2.28.0"
  },
  "devDependencies": {
    "@ovh-ux/manager-webpack-config": "^3.0.1",
    "cypress": "^3.1.3",
    "eslint-plugin-cypress": "^2.1.3",
    "ovh-manager-webfont": "^1.0.2",
    "webpack-merge": "^4.1.4"
  }
}
