{
  "name": "@ovh-ux/libs",
  "version": "0.0.0",
  "private": true,
  "description": "OVHcloud dependencies.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/manager/apps/libs"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "scripts": {
    "build": "mkdir dist && cp ./angular.min.js dist/ ",
