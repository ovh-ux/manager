{
  "name": "@ovh-ux/manager-private-database",
  "version": "0.0.0",
  "description": "OVH private database control panel",
  "keywords": [
    "control",
    "database",
    "manager",
    "ovh",
    "private"
  ],
  "license": "BSD-3-Clause",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/private-database.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-private-database' --include-filtered-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-private-database' --include-filtered-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-private-database' --include-filtered-dependencies -- yarn run dev"
  },
  "dependencies": {
    "filesize": "^3.5.10",
    "lodash": "3.10.1",
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "5.0.0-beta.5"
  },
  "peerDependencies": {
    "@ovh-ux/ng-ovh-web-universe-components": "^2.0.0",
    "@ovh-ux/ng-pagination-front": "8.0.0-alpha.0",
    "@ovh-ux/ng-tail-logs": "^2.0.0-beta.0",
    "@ovh-ux/ovh-utils-angular": "^14.0.0",
    "@uirouter/angularjs": "^1.0.15",
    "angular": "1.7.5",
    "angularjs-scroll-glue": "^2.2.0",
    "bootstrap": "^3.3.6",
    "bootstrap4": "twbs/bootstrap#v4.0.0",
    "chart.js": "^2.7.2",
    "font-awesome": "4.7.0",
    "jquery": "^2.1.3",
    "ovh-angular-responsive-tabs": "^4.0.0",
    "ovh-ui-angular": "^2.24.1",
    "ovh-ui-kit": "^2.23.1",
    "ovh-ui-kit-bs": "^2.0.1"
  }
}
