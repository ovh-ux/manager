{
  "name": "@ovh-ux/manager-kubernetes",
  "description": "OVH Kubernetes product control panel",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh-ux/manager.git",
    "directory": "packages/manager/modules/kubernetes"
  },
  "license": "BSD-3-Clause",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/welcome.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-kubernetes' --include-filtered-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-kubernetes' --include-filtered-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-kubernetes' --include-filtered-dependencies -- yarn run dev:watch"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^5.0.0-beta.9"
  },
  "peerDependencies": {
    "@ovh-ux/manager-core": "^4.0.0",
    "@uirouter/angularjs": "^1.0.22",
    "angular": "^1.7.8",
    "oclazyload": "^1.1.0"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
