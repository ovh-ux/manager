{
  "name": "@ovh-ux/manager-welcome",
  "version": "1.0.0",
  "license": "BSD-3-Clause",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "start": "lerna run build --stream --scope='@ovh-ux/manager-welcome' --include-filtered-dependencies",
    "start:dev": "lerna run dev --stream --scope='@ovh-ux/manager-welcome' --include-filtered-dependencies",
    "start:watch": "lerna run dev:watch --stream --parallel --scope='@ovh-ux/manager-welcome' --include-filtered-dependencies",
    "prepare": "yarn run build"
  },
  "browser": "./dist/umd/welcome.js",
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "5.0.0-beta.3"
  },
  "peerDependencies": {
    "@ovh-ux/manager-core": "^1.1.0",
    "@uirouter/angularjs": "^1.0.15",
    "angular": "~1.6.10",
    "oclazyload": "^1.1.0"
  }
}
