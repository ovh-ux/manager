{
  "name": "@ovh-ux/manager-telecom-styles",
  "version": "2.0.0",
  "license": "BSD-3-Clause",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "start": "lerna run build --stream --scope='@ovh-ux/manager-telecom-styles' --include-filtered-dependencies",
    "start:dev": "lerna run dev --stream --scope='@ovh-ux/manager-telecom-styles' --include-filtered-dependencies",
    "start:watch": "lerna run dev:watch --stream --parallel --scope='@ovh-ux/manager-telecom-styles' --include-filtered-dependencies",
    "prepare": "yarn run build"
  },
  "browser": "./dist/umd/index.js",
  "dependencies": {
    "bootstrap": "~3.3.7",
    "ovh-ui-kit": "^2.23.1",
    "ovh-ui-kit-bs": "^2.0.1"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "5.0.0-beta.3"
  }
}
