{
  "name": "@ovh-ux/ng-ovh-contracts",
  "version": "3.0.1",
  "description": "Component to correctly show contracts in OVH control panels",
  "keywords": [
    "angular",
    "contracts",
    "ovh"
  ],
  "repository": "ovh-ux/ng-ovh-contracts",
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/ng-ovh-contracts.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/ng-ovh-contracts' --include-filtered-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/ng-ovh-contracts' --include-filtered-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/ng-ovh-contracts' --include-filtered-dependencies -- yarn run dev:watch"
  },
  "dependencies": {
    "animated-scroll-to": "^1.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@ovh-ux/component-rollup-config": "^7.0.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-markdown": "^1.0.1",
    "htmlhint": "^0.11.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0"
  },
  "peerDependencies": {
    "angular": "1.6.10",
    "angular-translate": "^2.17.0",
    "angular-ui-bootstrap": "~1.3.3",
    "jquery": "^2.1.3"
  }
}
