{
  "name": "@ovh-ux/order-tracking",
  "version": "0.0.0",
  "description": "OVH order tracking",
  "keywords": [
    "angular",
    "order",
    "ovh",
    "tracking"
  ],
  "homepage": "https://github.com/ovh-ux/manager/tree/master/packages/components/order-tracking#readme",
  "bugs": {
    "url": "https://github.com/ovh-ux/manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh-ux/manager.git",
    "directory": "packages/components/order-tracking"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/order-tracking.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/order-tracking' --include-filtered-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/order-tracking' --include-filtered-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/order-tracking' --include-filtered-dependencies -- yarn run dev:watch"
  },
  "dependencies": {
    "bootstrap4": "twbs/bootstrap#v4.0.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^7.0.0"
  },
  "peerDependencies": {
    "angular": "^1.7.5",
    "ovh-api-services": "^9.22.0",
    "ovh-ui-angular": "^3.9.2",
    "ovh-ui-kit": "^2.35.2",
    "ovh-ui-kit-bs": "^3.0.0"
  }
}
