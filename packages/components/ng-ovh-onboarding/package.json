{
  "name": "@ovh-ux/ng-ovh-onboarding",
  "version": "0.0.0",
  "description": "Display a onboarding page when no records found.",
  "keywords": [
    "angularjs",
    "onboarding",
    "empty"
  ],
  "homepage": "https://github.com/ovh-ux/manager/tree/master/packages/components/ng-ovh-onboarding#readme",
  "bugs": {
    "url": "https://github.com/ovh-ux/manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/components/ng-ovh-onboarding"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/ng-ovh-onboarding.js",
  "files": [
    "dist",
    "examples"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/ng-ovh-onboarding' --include-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/ng-ovh-onboarding' --include-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/ng-ovh-onboarding' --include-dependencies -- yarn run dev:watch"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^7.0.0"
  },
  "peerDependencies": {
    "@uirouter/angularjs": "^1.0.22",
    "angular": "^1.5.0",
    "angular-translate": "^2.18.1",
    "@ovh-ux/ng-translate-async-loader": "^2.1.0",
    "@ovh-ux/ui-kit": "^4.1.12"
  }
}
